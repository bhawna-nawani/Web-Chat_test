    <script type='text/javascript'>
	function initEmbeddedMessaging() {
		try {
			embeddedservice_bootstrap.settings.language = 'en';

			embeddedservice_bootstrap.init(
				'00D040000004eI2',
				'SNHU_Messaging_in_app_and_web',
				'https://unify-snhu--dev.sandbox.my.site.com/ESWSNHUMessaginginapp1669136110432',
				{
					scrt2URL: 'https://unify-snhu--dev.sandbox.my.salesforce-scrt.com'
				}
			);
		} catch (err) {
			console.error('Error loading Embedded Messaging: ', err);
		}
	};
</script>
<script type='text/javascript' src='https://unify-snhu--dev.sandbox.my.site.com/ESWSNHUMessaginginapp1669136110432/assets/js/bootstrap.min.js' onload='initEmbeddedMessaging()'></script>

